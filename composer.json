{
    "name": "bangnokia/laravel-serve-livereload",
    "description": "Bring livereload to laravel artisan serve command",
    "license": "MIT",
    "authors": [
        {
            "name": "bangnokia",
            "email": "bangnokia@gmail.com"
        }
    ],
    "require": {
        "illuminate/console": "^7.0",
        "cboden/ratchet": "^0.4",
        "symfony/process": "^5.1@dev",
        "yosymfony/resource-watcher": "^2.0@dev"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0"
    },
    "autoload": {
        "psr-4": {
          "BangNokia\\ServeLiveReload\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BangNokia\\ServeLiveReload\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "BangNokia\\ServeLiveReload\\CommandServiceProvider",
                "BangNokia\\ServeLiveReload\\ResponseServiceProvider"
            ]
        }
    }
}
